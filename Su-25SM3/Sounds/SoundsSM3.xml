		<engine>
			<name>Thrust Low Freq Exterior</name>
			<path>Aircraft/Su-25/Sounds/Samples/Engine-Low-Freq.wav</path>
			<mode>looped</mode>
			
			<condition>
				<property>/engines/engine[0]/running</property>
				<not>
					<property>/sim/current-view/internal</property>
					<property>sim/crashed</property>
					<value type="bool">true</value>
				</not>
			</condition>
			
			<property>engines/engine[0]/thrust-lbs</property>
			
			<volume>
				<property>engines/engine[0]/thrust-lbs</property>
				<factor>0.008</factor>
				<min>0</min>
				<max>4.9</max>
			</volume>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>
		
		<engine>
			<name>Thrust Low Freq Interior</name>
			<path>Aircraft/Su-25/Sounds/Samples/Engine-Low-Freq.wav</path>
			<mode>looped</mode>
			
			<condition>
				<and>
					<property>/engines/engine[0]/running</property>
					<property>/sim/current-view/internal</property>
				</and>
				<not>
					<property>sim/crashed</property>
					<value type="bool">true</value>
				</not>
			</condition>
			
			<property>engines/engine[0]/thrust-lbs</property>
			
			<volume>
				<property>engines/engine[0]/thrust-lbs</property>
				<factor>0.00003</factor>
				<min>0</min>
				<max>2.9</max>
			</volume>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>

		<shutdown>
            <name>shutdown</name>
            <path>Aircraft/Su-25/Sounds/Samples/shutdown.wav</path>
			<mode>once</mode>
            <condition>
				<property>/controls/engines/engine[0]/cutoff</property>
                <or>
					<and>
						<greater-than>  
                        <property>/canopy/position-norm</property>
                        <value>0.0</value>
                		</greater-than>
						<property>/sim/current-view/internal</property>
					</and>
					<not>
                    	<property>/sim/current-view/internal</property>
                	</not>
				</or>
                <equals>
                    <property>engines/engine[0]/running</property>
                    <value>false</value>
                </equals>
				<greater-than>  
                        <property>/engines/engine[0]/rpm</property>
                        <value>0.1</value>
                </greater-than>
            </condition>
			<volume>
				<property>engines/engine[0]/rpm</property>
				<factor>0.1</factor>
				<min>0</min>
				<max>1.0</max>
			</volume>
			<reference-dist>100</reference-dist>
            <max-dist>1000</max-dist>
        </shutdown>
        
        
