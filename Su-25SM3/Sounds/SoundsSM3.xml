			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.001</factor>
			</volume>
			
		</flyby>
		
		<!-- Enviroment -->
		
		<wind>
			<name>Wind</name>
			<mode>looped</mode>
			<path>Sounds/wind.wav</path>
			
			<condition>
                    <or>
					<and>
						<greater-than>  
                        	<property>/canopy/position-norm</property>
                        	<value>0.0</value>
                		</greater-than>
						<property>/sim/current-view/internal</property>
					</and>
						<not>
                    		<property>/sim/current-view/internal</property>
                		</not>
					</or>
            </condition>
			
			<property>/velocities/airspeed-kt</property>
			
			<volume>
				<property>/position/altitude-ft</property>
				<factor>-0.000015</factor>
				<offset>1.0</offset>
				<min>0.1</min>
				<max>0.7</max>
			</volume>
			
			<volume>
				<property>/velocities/airspeed-kt</property>
				<factor>0.015</factor>
				<min>0.03</min>
				<max>0.55</max>
			</volume>
			
			<pitch>
				<property>/velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<offset>0.75</offset>
			</pitch>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</wind>
		
		<rain>
      		<name>rain</name>
      		<mode>looped</mode>
      		<path>Aircraft/Su-25/Sounds/Samples/rain.wav</path>
      		<type>avionics</type>
      		<condition>
        		<greater-than>
          			<property>/environment/rain-norm</property>
          			<value>0</value>
        		</greater-than>
        		<or>
          		<not>
            		<property>/sim/current-view/internal</property>
          		</not>
          		<greater-than>
            		<property>/canopy/position-norm</property>
            		<value>0</value>
          		</greater-than>
        		</or>
      		</condition>
      		<volume>
        		<property>velocities/airspeed-kt</property>
        		<factor>0.00075</factor>
        		<min>0.03</min>
        		<max>0.40</max>
      		</volume>
    	</rain>
    
    	<rain-windshield>
            <name>rain-windshield</name>
            <mode>looped</mode>
            <path>Aircraft/Su-25/Sounds/Samples/rain-windshield.wav</path>
            <type>avionics</type>
            <condition>
                <and>
                    <property>/sim/current-view/internal</property>
                    <greater-than>
                        <property>/environment/rain-norm</property>
                        <value>0</value>
                    </greater-than>
                </and>
            </condition>
            <volume>
                <property>velocities/airspeed-kt</property>
                <factor>0.00075</factor>
                <min>0.03</min>
                <max>0.40</max>
            </volume>
        </rain-windshield>

		<switch>
            <name>switch-click</name>
            <mode>once</mode>
            <path>Aircraft/Su-25/Sounds/Samples/switchclick2.wav</path>
            <condition>
              <and>
                <property>/controls/power/sound/click-switch</property>
                <property>/sim/current-view/internal</property>
              </and>
            </condition>
            <!-- <position>
                <x>-0.3660</x>
                <y>0.3350</y>
                <z>-0.3500</z>
            </position>
            <reference-dist>0.2</reference-dist>
            <max-dist>5.0</max-dist> -->
      	</switch>

		<switch>
            <name>mfd-click</name>
            <mode>once</mode>
            <path>Aircraft/Su-25/Sounds/Samples/mfdclick.wav</path>
            <condition>
              <and>
                <property>/controls/power/sound/click-switch2</property>
                <property>/sim/current-view/internal</property>
              </and>
            </condition>
            <!-- <position>
                <x>-0.3660</x>
                <y>0.3350</y>
                <z>-0.3500</z>
            </position>
            <reference-dist>0.2</reference-dist>
            <max-dist>5.0</max-dist> -->
      	</switch>
		
	</fx>
	
</PropertyList>
