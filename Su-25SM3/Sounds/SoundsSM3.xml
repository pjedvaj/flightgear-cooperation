		<engine>
			<name>Turbine1</name>
			<mode>looped</mode>
			<!-- <path>Aircraft/Su-25/Sounds/Samples/Engine-Turbine.wav</path> -->
			<path>Aircraft/Su-25/Sounds/Samples/Su25BackEngine.wav</path>
			
			<condition>
				<and>
				<property>engines/engine[0]/running</property>
                <not>
                    <!-- <property>/sim/current-view/internal</property> -->
                    <property>/engines/engine/out-of-fuel</property>
					<property>sim/crashed</property>
                </not>
				<or>
					<and>
						<greater-than>  
                        <property>/canopy/position-norm</property>
                        <value>0.0</value>
                		</greater-than>
						<property>/sim/current-view/internal</property>
					</and>
					<not>
                    	<property>/sim/current-view/internal</property>
                	</not>
				</or>
				</and>
			</condition>

				
			
			<position>
    			<x>4.0</x>
				<y>0.3</y>
				<z>0.0</z>
   			</position>
   			<orientation>
				<x>90.0</x>
    			<y>0.0</y>
    			<z>0.0</z>
    			<inner-angle>80.0</inner-angle>
   				<outer-angle>110.0</outer-angle>
   				<outer-gain>0.5</outer-gain>
   			</orientation>
			
			<volume>
				<property>engines/engine[0]/n2</property>
				<factor>0.01</factor>
				<min>0.2</min>
				<max>0.8</max>
			</volume>
			
			<pitch>
				<property>engines/engine[0]/n2</property>
				<factor>0.01</factor>
				<min>0</min>
				<max>0.9</max>
			</pitch>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>

