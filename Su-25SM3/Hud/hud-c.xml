  <label>
    <name>VSup</name>
    <x>41</x>
    <y>40</y>
    <width>12</width>
    <height>10</height>
    <input>
      <property>autopilot/internal/vert-speed-fpm</property>
      <factor>0.000305</factor>
      <max>10</max>
      <min>-10</min>
    </input>
    <condition>
      <property>/controls/power/battery</property>
      <greater-than>
        <property>autopilot/internal/vert-speed-fpm</property>
        <value>0</value>
      </greater-than>
    </condition>
    <format>%02d</format>
    <halign>right</halign>
    <box>true</box>
    <option>top</option>
  </label>

  <label>
    <name>VSdown</name>
    <x>41</x>
    <y>40</y>
    <width>12</width>
    <height>10</height>
    <input>
      <property>autopilot/internal/vert-speed-fpm</property>
      <factor>0.000305</factor>
      <max>10</max>
      <min>-10</min>
    </input>
    <condition>
      <property>/controls/power/battery</property>
      <less-than>
        <property>autopilot/internal/vert-speed-fpm</property>
        <value>0</value>
      </less-than>
    </condition>
    <format>%02d</format>
    <halign>right</halign>
    <box>true</box>
    <option>bottom</option>
  </label>

  <tape>
        <name>PITCH</name>
        <x>-50</x>
        <y>-10</y>
        <width>10</width>
        <height>50</height>
        <input>
            <property>/orientation/pitch-deg</property>
            <factor>1</factor>
            <max>90</max>
            <min>-90</min>
        </input>
        <major-divisions>10</major-divisions>
        <minor-divisions>5</minor-divisions>
        <display-span>30</display-span>
        <option>left</option>
        <option>vertical</option>
        <tick-bottom>false</tick-bottom>
        <tick-top>false</tick-top>
        <tick-right>false</tick-right>
        <tick-left>false</tick-left>
        <cap-bottom>false</cap-bottom>
        <cap-top>false</cap-top>
        <cap-right>true</cap-right>
        <cap-left>false</cap-left>
        <marker-offset>4</marker-offset>
        <enable-pointer>true</enable-pointer>
        <pointer-type>fixed</pointer-type>

      <condition>
        <property>/controls/power/battery</property>
        <equals>
          <property>controls/hud/hudmode</property>
          <value>4</value>
        </equals>
      </condition>
      
    </tape>

  <tape>
    <name>VS</name>
    <x>40</x>
    <y>-10</y>
    <width>10</width>
    <height>50</height>
    <input>
      <property>autopilot/internal/vert-speed-fpm</property>
      <factor>0.000305</factor>
      <max>10</max>
      <min>-10</min>
    </input>
    <major-divisions>1</major-divisions>
    <minor-divisions>0.5</minor-divisions>
    <display-span>3</display-span>
    <option>right</option>
    <option>vertical</option>
    <tick-bottom>false</tick-bottom>
    <tick-top>false</tick-top>
    <tick-right>false</tick-right>
    <tick-left>false</tick-left>
    <cap-bottom>false</cap-bottom>
    <cap-top>false</cap-top>
    <cap-right>false</cap-right>
    <cap-left>true</cap-left>
    <marker-offset>4</marker-offset>
    <enable-pointer>true</enable-pointer>
    <pointer-type>fixed</pointer-type>

    <condition>
      <property>/controls/power/battery</property>
      <equals>
        <property>controls/hud/hudmode</property>
        <value>4</value>
      </equals>
    </condition>

  </tape>

