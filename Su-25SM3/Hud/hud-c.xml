<?xml version="1.0"?>

<PropertyList>

  <enable3d>true</enable3d>


  <tape>
    <name>Gyrocompass</name>
    <x>-25</x>
    <y>69</y>
    <width>50</width>
    <height>12</height>
    <input>
      <!--<property>orientation/heading-deg</property>-->
      <property>orientation/heading-magnetic-deg</property>
      <factor>0.1</factor>
      <max>85</max>
      <min>0</min>
    </input>
    <condition>
      <property>/controls/power/battery</property>
    </condition>
    <display-span>3</display-span>
    <option>top</option>
    <major-divisions>1</major-divisions>
    <minor-divisions>0.5</minor-divisions>
    <tick-bottom>false</tick-bottom>
    <tick-top>false</tick-top>
    <tick-right>false</tick-right>
    <tick-left>false</tick-left>
    <cap-bottom>true</cap-bottom>
    <cap-top>false</cap-top>
    <cap-right>true</cap-right>
    <cap-left>true</cap-left>
    <marker-offset>0</marker-offset>
    <enable-pointer>false</enable-pointer>
    <pointer-type>fixed</pointer-type>
    <tick-type>line</tick-type>
    <tick-length>variable</tick-length>
  </tape>

  <label>
    <name>Gyro Heading Marker</name>
    <x>0</x>
    <y>62</y>
    <width>1</width>
    <height>1</height>
    <format>^</format>
    <condition>
      <property>/controls/power/battery</property>
    </condition>
  </label>

  <!-- <label>
    <name>Gyro Heading Label</name>
    <x>-9</x>
    <y>78</y>
    <width>18</width>
    <height>8</height>
    <input>
      <property>orientation/heading-magnetic-deg</property>
    </input>
    <condition>
      <property>/controls/power/battery</property>
    </condition>
    <format>%4.0f</format>
    <halign>center</halign>
    <box>true</box>
  </label> -->

  <label>
    <name>KIAS</name>
    <x>-58</x>
    <y>70</y>
    <width>25</width>
    <height>10</height>
    <input>
      <property>/velocities/airspeed-kt</property>
      <factor>1.852</factor>
    </input>
    <condition>
      <property>/controls/power/battery</property>
    </condition>
    <format>%4.0f</format>
    <halign>center</halign>
    <box>false</box>
    <option>right</option>
  </label>

  <label>
    <name>Altitude</name>
    <x>30</x>
    <y>70</y>
    <width>28</width>
    <height>10</height>
    <input>
      <property>/position/altitude-ft</property>
      <factor>0.305</factor>
    </input>
    <condition>
      <property>/controls/power/battery</property>
    </condition>
    <format>%4.0f</format>
    <halign>center</halign>
    <box>false</box>
    <option>left</option>
  </label>

  <label>
    <name>AutopilotAltitude</name>
    <x>30</x>
    <y>58</y>
    <width>28</width>
    <height>10</height>
    <input>
      <property>/autopilot/settings/target-altitude-ft</property>
      <factor>0.305</factor>
    </input>
    <condition>
        <property>/controls/power/battery</property>
      <equals>
        <property>/autopilot/locks/altitude</property>
        <value>altitude-hold</value>
      </equals>
    </condition>
    <format>%04d</format>
    <halign>right</halign>
    <box>false</box>
    <option>left</option>
  </label>

  <label>
    <name>AutopilotAltitudeR</name>
    <x>30</x>
    <y>58</y>
    <width>28</width>
    <height>10</height>
    <input>
      <property>/autopilot/settings/target-agl-ft</property>
      <factor>0.305</factor>
    </input>
    <condition>
      <property>/controls/power/battery</property>
      <equals>
        <property>/autopilot/locks/altitude</property>
        <value>agl-hold</value>
      </equals>
    </condition>
    <format>%04d p</format>
    <halign>right</halign>
    <box>false</box>
    <option>left</option>
  </label>

  <label>
    <name>AutopilotSpeed</name>
    <x>-58</x>
    <y>58</y>
    <width>28</width>
    <height>10</height>
    <input>
      <property>/autopilot/settings/target-speed-kt</property>
      <factor>1.852</factor>
    </input>
    <condition>
      <property>/controls/power/battery</property>
      <equals>
        <property>/autopilot/locks/speed</property>
        <value>speed-with-throttle</value>
      </equals>
    </condition>
    <format>%04d</format>
    <halign>left</halign>
    <box>false</box>
    <option>right</option>
  </label>

  <ladder>
    <name>Target Marker</name>
    <type>pitch</type>
    <x>-15</x>
    <y>5</y>
    <width>30</width>
    <height>20</height>
    <roll-input>
      <property>/orientation/roll-deg</property>
    </roll-input>
    <pitch-input>
      <property>/orientation/pitch-deg</property>
    </pitch-input>
    <compression-factor>7.8</compression-factor>

    <screen-hole>16</screen-hole>
    <zero-bar-overlength>15</zero-bar-overlength>
    <enable-dive-bar-angle>0</enable-dive-bar-angle>
    <enable-dynamic-origin>0</enable-dynamic-origin>
    <enable-fuselage-ref-line>0</enable-fuselage-ref-line>
    <enable-target-spot>false</enable-target-spot>
    <enable-target-markers>true</enable-target-markers>
    <enable-velocity-vector>0</enable-velocity-vector>
    <enable-drift-marker>0</enable-drift-marker>
    <enable-alpha-bracket>0</enable-alpha-bracket>
    <enable-energy-marker>0</enable-energy-marker>
    <enable-climb-dive-marker>0</enable-climb-dive-marker>
    <enable-glide-slope-marker>0</enable-glide-slope-marker>
    <glide-slope>0.0</glide-slope>
    <enable-energy-worm>0</enable-energy-worm>
    <enable-waypoint-marker>0</enable-waypoint-marker>

    <condition>
      <property>/controls/power/battery</property>
      <or>
        <equals>
          <property>controls/hud/hudmode</property>
          <value>1</value>
        </equals>
        <equals>
          <property>controls/hud/hudmode</property>
          <value>4</value>
        </equals>
      </or>
    </condition>
    
  </ladder>

  <ladder>
    <name>Nav Pitch Ladder</name>
    <type>pitch</type>
    <x>-40</x>
    <y>10</y>
    <width>80</width>
    <height>20</height>
    <roll-input>
      <property>/orientation/roll-deg</property>
    </roll-input>
    <pitch-input>
      <property>/orientation/pitch-deg</property>
    </pitch-input>
    <compression-factor>7.8</compression-factor>
    <display-span>7.5</display-span>
    <divisions>10.0</divisions>
    <screen-hole>40</screen-hole>
    <zero-bar-overlength>30</zero-bar-overlength>
    <enable-dive-bar-angle>0</enable-dive-bar-angle>
    <enable-dynamic-origin>0</enable-dynamic-origin>
    <enable-fuselage-ref-line>false</enable-fuselage-ref-line>
    <enable-target-spot>0</enable-target-spot>
    <enable-target-markers>false</enable-target-markers>
    <enable-velocity-vector>true</enable-velocity-vector>
    <enable-drift-marker>0</enable-drift-marker>
    <enable-alpha-bracket>0</enable-alpha-bracket>
    <enable-energy-marker>0</enable-energy-marker>
    <enable-climb-dive-marker>false</enable-climb-dive-marker>
    <enable-glide-slope-marker>false</enable-glide-slope-marker>
    <glide-slope>0.0</glide-slope>
    <enable-energy-worm>0</enable-energy-worm>
    <enable-waypoint-marker>true</enable-waypoint-marker>
    <enable-zenith>true</enable-zenith>
    <enable-nadir>true</enable-nadir>

    <condition>
      <property>/controls/power/battery</property>
      <less-than>
        <property>controls/hud/hudmode</property>
        <value>3</value>
      </less-than>
    </condition>

  </ladder>

