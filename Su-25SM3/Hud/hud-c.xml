  <label>
    <name>Alpha Label</name>
    <x>-55</x>
    <y>-21</y>
    <width>2</width>
    <height>2</height>
    <input>
      <property>/orientation/alpha-deg</property>
    </input>
    <format>a:%2.0f</format>
    <halign>left</halign>
    <box>false</box>
    <option>left</option>

    <condition>
      <property>/controls/power/battery</property>
      <not-equals>
        <property>controls/hud/hudmode</property>
        <value>4</value>
      </not-equals>
    </condition>
  </label>

  <label>
        <name>MACH Number</name>
        <x>-55</x>
        <y>-31</y>
       <width>2</width>
        <height>2</height>
        <input>
            <property>/velocities/mach</property>
        </input>
        <format>M:%4.2f</format>
        <halign>left</halign>
      
      <condition>
        <property>/controls/power/battery</property>
        <not-equals>
          <property>controls/hud/hudmode</property>
          <value>4</value>
        </not-equals>
      </condition>
      
  </label>

  <label>
    <name>G Load</name>
    <x>-55</x>
    <y>-41</y>
    <width>2</width>
    <height>2</height>
    <input>
      <property>/accelerations/pilot/z-accel-fps_sec</property>
      <factor>-0.03108095</factor>
      <damp>1.0</damp>
    </input>
    <format>G:%2.1f</format>
    <halign>left</halign>

    <condition>
      <property>/controls/power/battery</property>
      <not-equals>
        <property>controls/hud/hudmode</property>
        <value>4</value>
      </not-equals>
    </condition>

    <blinking>
      <interval>0.25</interval>
      <condition>
        <or>
          <less-than>
            <!-- G load > 3.0 -->
            <property>/accelerations/pilot/z-accel-fps_sec</property>
            <value>-93.52215</value>
          </less-than>
          <greater-than>
            <!-- G load < -1.0 -->
            <property>/accelerations/pilot/z-accel-fps_sec</property>
            <value>31.17405</value>
          </greater-than>
        </or>
      </condition>
    </blinking>

  </label>

  <label>
    <width>1</width>
    <height>1</height>
    <x>-1</x>
    <y>-48</y>
    <format>%4s</format>
    <halign>right</halign>
    <input>
      <property>/instrumentation/gps/wp/wp[1]/ID</property>
    </input>

    <condition>
      <property>/controls/power/battery</property>
      <and>
        <property>/instrumentation/gps/wp/wp[1]/valid</property>
        <equals>
          <property>/instrumentation/gps/mode</property>
          <value>leg</value>
        </equals>
        <equals>
          <property>controls/hud/hudmode</property>
          <value>0</value>
        </equals>
      </and>
    </condition>

  </label>

  <label>
    <width>1</width>
    <height>1</height>
    <x>1</x>
    <y>-48</y>
    <format>%4.1f</format>
    <halign>left</halign>
    <input>
      <property>/instrumentation/gps/wp/wp[1]/distance-nm</property>
    </input>

    <condition>
      <property>/controls/power/battery</property>
      <and>
        <property>/instrumentation/gps/wp/wp[1]/valid</property>
        <equals>
          <property>/instrumentation/gps/mode</property>
          <value>leg</value>
        </equals>
        <equals>
          <property>controls/hud/hudmode</property>
          <value>0</value>
        </equals>
      </and>
    </condition>
    
  </label>

  <label>
        <name>Radio Alt</name>
        <x>58</x>
        <y>-21</y>
        <width>2</width>
        <height>2</height>
        <input>
          <property>/position/altitude-agl-ft</property>
          <factor>0.305</factor>
          <min>0.0</min>
        </input>
        <format>%4.0f p</format>
        <halign>right</halign>
      
      <condition>
        <property>/controls/power/battery</property>
        <and>
        <not-equals>
          <property>controls/hud/hudmode</property>
          <value>3</value>
        </not-equals>
        <not>
          <or>
            <less-than>
              <property>orientation/roll-deg</property>
              <value>-60</value>
            </less-than>
            <greater-than>
              <property>orientation/roll-deg</property>
              <value>60</value>
            </greater-than>
            <less-than>
              <property>orientation/pitch-deg</property>
              <value>-60</value>
            </less-than>
            <greater-than>
              <property>orientation/pitch-deg</property>
              <value>60</value>
            </greater-than>
            <greater-than>
              <property>position/altitude-agl-ft</property>
              <value>3000</value>
            </greater-than>
          </or>
        </not>
        </and>
      </condition>
  </label>

  <!--STATION MARKERS-->


  <label>
    <name> Pylon 2</name>
    <x>-59</x>
    <y>-58</y>
    <width>11</width>
    <height>8</height>
    <format>2</format>
    <box>true</box>

    <condition>
      <property>/controls/power/battery</property>
      <and>
        <not-equals>
          <property>sim/weight[1]/selected</property>
          <value>none</value>
        </not-equals>
        <greater-than>
          <property>controls/hud/hudmode</property>
          <value>1</value>
        </greater-than>
          <or>
	          <and>
              <equals>
                <property>controls/armament/selectedweapon</property>
                <value>2</value>
              </equals>
	            <equals>
                <property>sim/weight[1]/selected</property>
                <value>S-8 rockets</value>
              </equals>
	          </and>
	          <and>
              <equals>
                <property>controls/armament/selectedweapon</property>
                <value>3</value>
              </equals>
	            <equals>
                <property>sim/weight[1]/selected</property>
                <value>FAB-250 bomb</value>
              </equals>
	          </and>
	        </or>
      </and>
    </condition>
  </label>

  <label>
    <name> Pylon 3</name>
    <x>-44</x>
    <y>-58</y>
    <width>11</width>
    <height>8</height>
    <format>3</format>
    <box>true</box>

    <condition>
      <property>/controls/power/battery</property>
      <and>
        <not-equals>
          <property>sim/weight[2]/selected</property>
          <value>none</value>
        </not-equals>
        <greater-than>
          <property>controls/hud/hudmode</property>
          <value>1</value>
        </greater-than>
          <or>
	          <and>
