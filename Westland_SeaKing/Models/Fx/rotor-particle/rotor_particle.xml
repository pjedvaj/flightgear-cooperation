<?xml version="1.0"?><PropertyList><!-- == MAIN ROTOR               == --><!-- == PARTICLE SYSTEM for SH-3 == --><!-- ==                          == --><!-- Translate & Rotate -->	<animation>		<name>dirt_front</name>			<object-name>dirt_02_front</object-name>	</animation>			<animation>		<type>rotate</type>		<object-name>dirt_front</object-name>		<!-- <property>orientation/pitch-deg</property>  -->		<property>sim/multiplay/generic/float[1]</property>		<axis>			<x>0</x>			<y>-1</y>			<z>0</z>		</axis>	</animation>	<animation>		<type>rotate</type>		<object-name>dirt_front</object-name>		<!-- <property>orientation/roll-deg</property> -->		<property>sim/multiplay/generic/float[2]</property>		<axis>			<x>1</x>			<y>0</y>			<z>0</z>		</axis>	</animation>	<animation>		<type>translate</type>		<object-name>dirt_front</object-name>		<!-- <property>position/gear-agl-m</property> -->		<property>sim/multiplay/generic/float[0]</property>		<axis>			<x>0</x>			<y>0</y>			<z>-1</z>		</axis>	</animation>		<!-- Move particle system with rotor tilt - needs update for MP? 	<animation>		<object-name>dirt_front</object-name>               <property>rotors/main/roll-deg</property>		<factor>1</factor>		<center>			<x-m>-0.0</x-m>			<y-m>-0.0</y-m>			<z-m>1.812</z-m>		</center>		<axis>			<x>1</x>			<y>0</y>			<z>0</z>		</axis>	</animation>			<animation>		<object-name>dirt_front</object-name>               <property>rotors/main/yaw-deg</property>		<factor>1</factor>		<center>			<x-m>-0.0</x-m>			<y-m>-0.0</y-m>			<z-m>1.812</z-m>		</center>		<axis>			<x>0</x>			<y>1</y>			<z>0</z>		</axis>	</animation>	-->			<!-- MAIN ROTOR - Low GROUND rotor DIRT - particle system controled by engines-->	<particlesystem>		<name>dirt_02_front</name>		<texture>smoke_dirt.png</texture>		<emissive>false</emissive>		<lighting>true</lighting>		<offsets>			<x-m>-3.930 </x-m>			<y-m> 0.000</y-m>			<z-m>-2.480</z-m>			<roll-deg>90</roll-deg>		</offsets>		<attach>world</attach>		<placer>			<type>point</type>		</placer>		<condition>			<and>				<not>					<property>sim/model/rotor-particles</property>				</not>				<not>					<property>sim/paused</property>				</not>				<not>					<property>sim/crashed</property>				</not>				<less-than>					<!--					<property>position/gear-agl-m</property>					<value>30</value>					-->					<property>sim/multiplay/generic/float[0]</property>					<value>30</value>				</less-than>				<greater-than>					<property>rotors/main/rpm</property>					<value>25</value>					<!-- <value>50</value> -->				</greater-than>			</and>		</condition>		<shooter>			<theta-min-deg>0</theta-min-deg>			<theta-max-deg>360</theta-max-deg>			<phi-min-deg>-0.5</phi-min-deg>			<phi-max-deg>3.2</phi-max-deg>			<speed-mps>				<!--				<property>rotors/main/torque</property>				-->				<property>sim/multiplay/generic/float[3]</property>				<factor>1</factor>				<interpolation>					<entry><ind>     0.0 </ind><dep> 10.00 </dep></entry>					<entry><ind>  4000.0 </ind><dep> 20.00 </dep></entry>					<entry><ind> 80000.0 </ind><dep> 30.00 </dep></entry>				</interpolation>				<spread>50</spread>			</speed-mps>			<rotation-speed>				<x-max-deg-sec>15</x-max-deg-sec>				<x-min-deg-sec>-15</x-min-deg-sec>				<y-max-deg-sec>15</y-max-deg-sec>				<y-min-deg-sec>-15</y-min-deg-sec>				<z-max-deg-sec>360</z-max-deg-sec>				<z-min-deg-sec>-360</z-min-deg-sec>			</rotation-speed>		</shooter>		<counter>			<particles-per-sec>				<!--				<property>position/gear-agl-m</property>				-->				<property>sim/multiplay/generic/float[0]</property>				<interpolation>					<entry><ind>  0.0 </ind><dep> 60.00 </dep></entry>					<entry><ind> 15.0 </ind><dep> 15.00 </dep></entry>					<entry><ind> 30.0 </ind><dep>  0.00 </dep></entry>				</interpolation>			</particles-per-sec>		</counter>		<align>billboard</align>		<particle>			<start>				<color>					<red>						<value>0.95</value>					</red>					<green>						<value>0.95</value>					</green>					<blue>						<value>0.95</value>					</blue>					<alpha>						<property>rotors/main/rpm</property>						<factor>0.0004</factor>						<max>0.02</max>					</alpha>				</color>				<size>					<value>0.5</value>				</size>			</start>			<end>				<color>					<red>						<value>1</value>					</red>					<green>						<value>1</value>					</green>					<blue>						<value>1</value>					</blue>					<alpha>						<property>rotors/main/rpm</property>						<factor>0.0002</factor>						<max>0.5</max>					</alpha>				</color>								<size>					<!-- amount of Dust / end size of particle controlled by torque -->					<!--					<property>rotors/main/torque</property>					-->				<property>sim/multiplay/generic/float[3]</property>					<factor>1</factor>					<interpolation>						<entry><ind>     0.0 </ind><dep> 0.8 </dep></entry>						<entry><ind>  6000.0 </ind><dep> 1.4 </dep></entry>						<entry><ind>120000.0 </ind><dep>11.0 </dep></entry>					</interpolation>					<spread>0.4</spread>				</size>			</end>			<life-sec>				<!--				<property>rotors/main/torque</property>				-->				<property>sim/multiplay/generic/float[3]</property>				<factor>1</factor>				<interpolation>					<entry><ind>     0.0 </ind><dep> 0.80 </dep></entry>					<entry><ind>  6000.0 </ind><dep> 1.40 </dep></entry>					<entry><ind>120000.0 </ind><dep> 3.27 </dep></entry>				</interpolation>			</life-sec>						<mass-kg>0.035</mass-kg>			<radius-m>0.045</radius-m>		</particle>		<program>			<fluid>air</fluid>			<gravity>false</gravity>			<wind>true</wind>		</program>	</particlesystem></PropertyList>