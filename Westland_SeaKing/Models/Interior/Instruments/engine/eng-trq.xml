<?xml version="1.0"?>
<PropertyList>

    <path>eng-trq.ac</path>

 <animation>
  <type>scale</type>
  <x-offset>0.36</x-offset>
  <y-offset>0.36</y-offset>
  <z-offset>0.36</z-offset>
 </animation>

    <animation>
        <type>material</type>
        <object-name>ENGtrq.face</object-name>
        <object-name>ENGtrq.needle1</object-name>
        <object-name>ENGtrq.needle2</object-name>
        <emission>
            <red>0.0175</red>
            <green>0.0175</green>
            <blue>0.0175</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>ENGtrq.needle1</object-name>
        <property>rotors/main/torque</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>100000</ind><dep>330.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>ENGtrq.needle2</object-name>
        <property>rotors/tail/torque</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>100000</ind><dep>330.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>