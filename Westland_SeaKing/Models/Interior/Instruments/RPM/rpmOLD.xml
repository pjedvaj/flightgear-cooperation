<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>rpm.ac</path>

  <animation>
<object-name>fond</object-name>
<object-name>face</object-name>
<object-name>Lneedle</object-name>
<object-name>Rneedle</object-name>


</animation>



<animation>
<type>material</type>
<object-name>face</object-name>
<object-name>Lneedle</object-name>
<emission>
<red> 0.030 </red>
<green> 0.015 </green>
<blue> 0.005 </blue>
<factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
</emission>
</animation>

<animation>
<type>material</type>
<object-name>face</object-name>
<object-name>Rneedle</object-name>
<emission>
<red> 0.8 </red>
<green> 0.2 </green>
<blue> 0.00 </blue>
<factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
</emission>
</animation>

<animation>
<type>rotate</type>
<object-name>Lneedle</object-name>
<property>sim/model/uh1/torque-pct</property>
<interpolation>
<entry><ind> 0 </ind><dep> -260 </dep></entry>
<entry><ind> 220 </ind><dep> 80 </dep></entry>
<entry><ind> 440 </ind><dep> -160 </dep></entry>
</interpolation>
<axis>
<x> -1 </x>
<y> 0 </y>
<z> 0 </z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>Rneedle</object-name>
<property>rotors/main/rpm</property>
<interpolation>
<entry><ind> 0 </ind><dep> -260 </dep></entry>
<entry><ind> 220 </ind><dep> 8 </dep></entry>
<entry><ind> 400 </ind><dep> 70 </dep></entry>
</interpolation>
<axis>
<x> -1 </x>
<y> 0 </y>
<z> 0 </z>
</axis>
</animation>


</PropertyList>
