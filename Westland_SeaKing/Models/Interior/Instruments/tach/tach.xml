<?xml version="1.0"?>

<PropertyList>
	<path>tach.ac</path>
	
	 <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
    <condition>
      <property>sim/rendering/rembrandt/enabled</property>
    </condition>
       
	<object-name>Face</object-name>
	
  
    </effect> 

	<animation>
		<type>rotate</type>
		<object-name>TurbineNeedle1</object-name>
		<property>engines/engine[0]/n2-pct</property>
		<interpolation>
			<entry><ind>0</ind><dep>35</dep></entry>
			<entry><ind>50</ind><dep>151</dep></entry>
			<entry><ind>100</ind><dep>267</dep></entry>
			<entry><ind>140</ind><dep>360</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>TurbineNeedle2</object-name>
		<property>engines/engine[1]/n2-pct</property>
		<interpolation>
			<entry><ind>0</ind><dep>35</dep></entry>
			<entry><ind>50</ind><dep>151</dep></entry>
			<entry><ind>100</ind><dep>267</dep></entry>
			<entry><ind>140</ind><dep>360</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>RotorNeedle</object-name>
		<property>rotors/main/rpm</property>
		<interpolation>
			<entry><ind>0</ind><dep>35</dep></entry>
			<entry><ind>101.5</ind><dep>151</dep></entry>
			<entry><ind>203</ind><dep>267</dep></entry>
			<entry><ind>284.2</ind><dep>360</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
		</axis>
	</animation>
	
	 <animation>
    <type>material</type>
    <object-name>RotorNeedle</object-name>
	<object-name>TurbineNeedle1</object-name>
		<object-name>TurbineNeedle2</object-name>
		<object-name>Face</object-name>
    <object-name>needle</object-name>
   <emission>
            <red>   0.005 </red>
      <green> 1 </green>
      <blue>  0.015 </blue>
       <factor-prop>controls/lighting/instrument-lights-norm</factor-prop>
    </emission>
  </animation>
	
	
 
	
</PropertyList>

