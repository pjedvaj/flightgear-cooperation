<?xml version="1.0" encoding="ISO-8859-1"?> 

<PropertyList>

  <path>firetest.ac</path>

  <animation>
    <object-name>Switch</object-name>
    <object-name>base</object-name>
   
  </animation>

  <animation>
    <type>material</type>
    <object-name>Switch</object-name>
    <object-name>base</object-name>
    
    <emission>
       <red>0.001</red>
        <green>0.001</green>
        <blue>0.001</blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>
  
   
  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>Switch</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/electric/warningtest</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Switch</object-name>
   <property>controls/electric/warningtest</property>
    <factor> 35 </factor>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

 

</PropertyList>


