<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <!-- Engine 0 -->
    <filter>
        <name>engine0-ground-idle</name>
        <type>gain</type>
        <gain>1.0</gain>
        <input>
            <expression>
                <log10>
                    <table>
                        <property>/engines/engine[0]/n1-pct</property>
                        <entry>
                            <ind>0</ind>
                            <dep>1</dep>
                        </entry>
                        <entry>
                            <ind>30</ind>
                            <dep>10</dep>
                        </entry>
                        <entry>
                            <ind>61</ind>
                            <dep>10</dep>
                        </entry>
                        <entry>
                            <ind>73</ind>
                            <dep>1</dep>
                        </entry>
                    </table>
                </log10>
            </expression>
        </input>
        <output>
            <property>/sounds/H145/engines/engine0-ground-idle</property>
        </output>
    </filter>
