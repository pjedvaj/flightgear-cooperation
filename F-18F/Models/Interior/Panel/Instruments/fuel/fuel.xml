<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- From Helijah SR-71 -->
<PropertyList>

  <path>fuel.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>number1</object-name>
    <object-name>number10</object-name>
    <object-name>number100</object-name>
    <object-name>number1000</object-name>
    <object-name>number10000</object-name>
    <object-name>number100000</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>number1</object-name>
    <object-name>number10</object-name>
    <object-name>number100</object-name>
    <object-name>number1000</object-name>
    <object-name>number10000</object-name>
    <object-name>number100000</object-name>
    <object-name>vitre</object-name>
  </animation>

  <!-- working for Compositor -->
  <effect>
    <inherits-from>../../../../Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>number1</object-name>
    <object-name>number10</object-name>
    <object-name>number100</object-name>
    <object-name>number1000</object-name>
    <object-name>number10000</object-name>
    <object-name>number100000</object-name>
    <condition>
      <property>controls/lighting/instrument-lights</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>   1 </red>
      <green> 1 </green>
      <blue>  1 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>consumables/fuel/total-fuel-lbs</property>
    <condition>
      <property>/controls/electric/battery-switch</property>
    </condition>
    <interpolation>
      <entry><ind> 0 </ind><dep>   0 </dep></entry>
      <entry><ind> 44000 </ind><dep> 270 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>number1</object-name>
    <property>consumables/fuel/total-fuel-lbs</property>
    <condition>
      <property>/controls/electric/battery-switch</property>
    </condition>
    <factor>0.1</factor>
    <step>1</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>number10</object-name>
    <property>consumables/fuel/total-fuel-lbs</property>
    <condition>
      <property>/controls/electric/battery-switch</property>
    </condition>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>number100</object-name>
    <property>consumables/fuel/total-fuel-lbs</property>
    <condition>
      <property>/controls/electric/battery-switch</property>
    </condition>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>number1000</object-name>
    <property>consumables/fuel/total-fuel-lbs</property>
    <condition>
      <property>/controls/electric/battery-switch</property>
    </condition>
    <factor>0.0001</factor>
    <step>1000</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>number10000</object-name>
    <property>consumables/fuel/total-fuel-lbs</property>
    <condition>
      <property>/controls/electric/battery-switch</property>
    </condition>
    <factor>0.00001</factor>
    <step>10000</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>number100000</object-name>
    <property>consumables/fuel/total-fuel-lbs</property>
    <condition>
      <property>/controls/electric/battery-switch</property>
    </condition>
    <factor>0.000001</factor>
    <step>100000</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Alert</object-name>
    <condition>
      <and>
        <less-than>
          <property>consumables/fuel/total-fuel-lbs</property>
          <value>2000</value>
        </less-than>
        <property>/controls/electric/battery-switch</property>
      </and>
    </condition>
  </animation>

</PropertyList>
