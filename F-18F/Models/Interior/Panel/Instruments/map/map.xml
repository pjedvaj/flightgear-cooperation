  <!-- radar mode -->
  <animation>
    <type>pick</type>
    <object-name>modenoarc</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>instrumentation/radar/display-mode</property>
        <value>NOARC</value>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>modearc</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>instrumentation/radar/display-mode</property>
        <value>ARC</value>
      </binding>
    </action>
  </animation>

  <!-- radar range -->
  <animation>
    <type>pick</type>
    <object-name>plus</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/radar[0]/range</property>
        <step> 10 </step>
        <min>  10 </min>
        <max> 100 </max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>moins</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/radar[0]/range</property>
        <step> -10 </step>
        <min>   10 </min>
        <max>  100 </max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>num10</object-name>
