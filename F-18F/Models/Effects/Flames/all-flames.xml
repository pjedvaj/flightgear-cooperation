<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <model>
    <path>combustionL.xml</path>
    <condition>
      <not><property>/controls/engines/engine[0]/cutoff</property></not>
    </condition>
    <offsets>
      <x-m>  7.48 </x-m>
      <y-m> -0.48 </y-m>
      <z-m>  2.21 </z-m>
    </offsets>
    <overlay>
      <animation n="100">
        <type>scale</type>
        <property>constants/zero</property>
        <x-offset> 0.80 </x-offset>
        <y-offset> 0.55 </y-offset>
        <z-offset> 0.55 </z-offset>
      </animation>
    </overlay>
  </model>

  <model>
    <path>combustionR.xml</path>
    <condition>
      <not><property>/controls/engines/engine[1]/cutoff</property></not>
    </condition>
    <offsets>
      <x-m>  7.48 </x-m>
      <y-m>  0.48 </y-m>
      <z-m>  2.21 </z-m>
    </offsets>
    <overlay>
      <animation n="100">
        <type>scale</type>
        <property>constants/zero</property>
        <x-offset> 0.80 </x-offset>
        <y-offset> 0.55 </y-offset>
        <z-offset> 0.55 </z-offset>
      </animation>
    </overlay>
  </model>

</PropertyList>
