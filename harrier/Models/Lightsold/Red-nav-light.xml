<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

<model>
	<name>red-nav-normal</name>
	<path>Aircraft/harrier/Models/Lights/Normal/red-nav-norm.xml</path>
</model>

<model>
	<name>red-nav-rembrandt</name>
	<path>Aircraft/harrier/Models/Lights/Rembrandt/red-nav-remb.xml</path>
</model>

<animation>
	<type>select</type>
    	<object-name>red-nav-normal</object-name>
   	<condition>
		<not>
      			<property>/sim/rendering/rembrandt/enabled</property>
		</not>
   	</condition>
</animation>

<animation>
	<type>select</type>
    	<object-name>red-nav-rembrandt</object-name>
   	<condition>
      		<property>/sim/rendering/rembrandt/enabled</property>
   	</condition>
</animation>

</PropertyList>