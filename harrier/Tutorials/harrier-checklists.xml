<PropertyList>

<checklist>  
	<title>Startup</title>
    	<item>
      		<name>Primary bus battery connection</name>
      		<value>On</value>
		<condition>
        		<equals>
          			<property>/controls/switches/battery</property>
				<value>true</value>
        		</equals>
      		</condition>
		<binding>
        		<command>property-assign</command>
        		<property>/controls/switches/battery</property>
			<value>true</value>
		</binding>
		<binding>
        		<command>property-assign</command>
        		<property>/instrumentation/attitude-indicator/serviceable</property>
			<value>true</value>
		</binding>
    	</item>
    	<item>
