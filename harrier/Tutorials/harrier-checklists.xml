        		<property>/controls/lighting/beacon</property>
			<value>true</value>
		</binding>
    	</item>
    	<item>
      		<name>Engine</name>
      		<value>On</value>
		<condition>
        		<greater-than>
          			<property>/engines/engine[0]/rpm</property>
				<value>50</value>
        		</greater-than>
      		</condition>
		<binding>
        		<command>nasal</command>
			<script>engines.Pegasus_mk106.autostart();</script>
		</binding>
		<marker>
        		<x-m>-15.5190</x-m>
